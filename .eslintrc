{
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "globals": {
        "$": true,
        "jQuery": true,
        "angular": true
    },
    "rules": {
        "brace-style": [2, "1tbs", { "allowSingleLine": true }],
        "comma-spacing": [1, { "before": false, "after": true }],
        "no-with": 1
    }
}
